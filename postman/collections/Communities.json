				"type": "text/javascript",
				"exec": [
					"const headerSchema = {",
					"    \"type\": \"object\",",
					"    \"properties\": {",
					"        \"result\": {",
					"            \"type\": \"object\",",
					"            \"properties\": {",
					"                \"has_error\": {",
					"                    \"type\": \"string\",",
					"                    \"maxLength\": 1",
					"                },",
					"                \"version\": {",
					"                    \"type\": \"string\",",
					"                    \"maxLength\": 1",
					"                },",
					"                \"expire\": {",
					"                    \"type\": \"string\",",
					"                    \"pattern\": \"^[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}(\\.[0-9]{1,3})?\"",
					"                },",
					"                \"request_name\": {",
					"                    \"type\": \"string\"",
					"                }",
					"            },",
					"            \"required\": [",
					"                \"has_error\",",
					"                \"version\",",
					"                \"request_name\"",
					"            ]",
					"        },",
					"",
					"    },",
					"    \"required\": [",
					"        \"result\"",
					"    ]",
					"};",
					"",
					"pm.test(\"Valid XML Response Header\", function () {",
					"    var json = xml2Json(pm.response.text());",
					"    pm.expect(json).to.have.jsonSchema(headerSchema);",
					"})"
				]
			}
		}
